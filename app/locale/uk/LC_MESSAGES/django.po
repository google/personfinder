#, python-format
msgid ""
"\n"
"            Search results for: \"%(query)s\"\n"
"          "
msgstr ""
"\n"
"            Результати пошуку за запитом \"%(query)s\"\n"
"          "

#, python-format
msgid ""
"\n"
"          Records Similar to: %(full_name)s\n"
"        "
msgstr ""
"\n"
"          Записи, подібні до імені \"%(full_name)s\"\n"
"        "

#, python-format
msgid ""
"\n"
"      Are you sure you want to delete the record for \"%(full_name)s\"?\n"
"    "
