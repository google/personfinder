#, python-format
msgid ""
"[Person Finder] Record restoration notice for \"%(first_name)s %(last_name)s"
"\""
msgstr ""
"[Cercador de persones] Avís de restauració de registre de \"%(first_name)s %"
"(last_name)s\""

#: subscribe.py:66
#, python-format
msgid "[Person Finder] Status update for %(given_name)s %(family_name)s"
msgstr ""
"[Cercador de persones] Actualització d'estat de %(given_name)s %(family_name)"
"s"

#: subscribe.py:90
#, python-format
msgid ""
"[Person Finder] You are subscribed to status updates for %(given_name)s %"
"(family_name)s"
msgstr ""
"[Cercador de persones] Esteu subscrit a les actualitzacions d'estat de %"
"(given_name)s %(family_name)s"

#: subscribe.py:139
msgid "Invalid e-mail address. Please try again."
msgstr "Adreça electrònica no vàlida. Torneu-ho a provar."

#: subscribe.py:148 subscribe.py:179
#, python-format
msgid "Return to the record for %(given_name)s %(family_name)s."
msgstr "Torna al registre de %(given_name)s %(family_name)s."

#: subscribe.py:153
msgid "You are already subscribed. "
msgstr "Ja esteu subscrit. "

#: subscribe.py:184
msgid "You have successfully subscribed."
msgstr "Us heu subscrit correctament."

#: unsubscribe.py:29
msgid "This link is invalid."
msgstr "Aquest enllaç no és vàlid."

#: unsubscribe.py:34
msgid "You have successfully unsubscribed."
msgstr "Heu anul·lat la subscripció correctament."

#: unsubscribe.py:36
msgid "You are already unsubscribed."
msgstr "Ja heu anul·lat la subscripció."

