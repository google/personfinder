<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% if static_map_files %}
  <sitemap>
    {% for static_map in static_map_files %}
      <loc>http://{{ domain }}/{{ static_map }}</loc>
      <lastmod>{{ static_lastmod }}</lastmod>
    {% endfor %}
  </sitemap>
  {% endif %}
  {% for shard in shards %}
  <sitemap>
    <loc>http://{{ domain }}/sitemap?shard_index={{ shard.index }}</loc>
    <lastmod>{{ shard.lastmod }}</lastmod>
  </sitemap>
  {% endfor %}
</sitemapindex>
